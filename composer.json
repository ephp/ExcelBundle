{
    "name": "ephp/office",
    "description": "This is a Symfony2 Bundle helps you to read and write Excel files (including pdf, xlsx, odt), thanks to the PHPExcel library",
    "authors": [
        {
            "name": "Giulio De Donato",
            "email": "liuggio@gmail.com"
        },
		{
            "name": "Ephraim Pepe",
            "email": "staff@jf-system.com"
        },
        {
            "name": "OfficeBundle Contributors",
            "homepage": "https://github.com/ephp/OfficeBundle#contributors"
        }
    ],

    "keywords": ["xls","Excel", "symfony2", "bundle"],
    "homepage": "http://www.welcometothebundle.com",
    "license": "MIT",
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "~2.1",
        "ephpoffice/phpexcel": "dev-master@dev",
        "ephpoffice/phpword": "dev-master"@dev"
    },
    "require-dev": {
        "phpunit/PHPUnit": "3.7.*",
        "symfony/finder": "~2.0",
        "symfony/form": "~2.1",
        "symfony/class-loader": "~2.1",
        "symfony/validator": "~2.1",
        "symfony/browser-kit": "~2.1",
        "sensio/framework-extra-bundle": "~2.1"
    },

    "autoload": {
        "psr-0": {
            "Ephp\\OfficeBundle": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "target-dir": "Ephp/OfficeBundle",
    "config": {
        "bin-dir": "bin"
    }
}
